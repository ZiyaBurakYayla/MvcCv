
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_Adminlayout.cshtml";
}
<br />
<h2>Yeni Deneyim Ekle</h2>
<br />
<form method="post">
    <input class="form-control" type="text" name="Title" placeholder="Başlığı yazınız"/>
    <br />
    <input class="form-control" type="text" name="Subheading" placeholder="Alt başlık" />
    <br />
    <textarea placeholder="Açıklama" name="Description" class="form-control" rows="4"></textarea>
    <br />
    <input class="form-control" type="text" name="Date"  placeholder="Tarih"/>
    <br />
    <button class="btn btn-primary" type="submit">Deneyim Ekle</button>
</form>

